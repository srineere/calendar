<html lang="en">
  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<!-- Latest compiled JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<body>

  <div class="container p-5">
    <h1 class="h1">Edit Event</h1>
    <form class="form-group" method="POST">
      <label for="title">Title:</label>
      <input class="form-control" type="text" id="title" name="title" value="<%= meet.title %>">
      <label for="description">Description:</label>
      <input class="form-control" id="description" name="description" value="<%= meet.description %>"></input>
      <label for="start">Start Time:</label>
      <input class="form-control" type="datetime-local" id="start" name="start" value="<%= meet.start %>" required>
      <label for="end">End Time:</label>
      <input class="form-control" type="datetime-local" id="end" name="end" value="<%= meet.end %>" required>
      <button class="btn btn-success">Save</button>
    </form>
  </div>
</body>
</html>